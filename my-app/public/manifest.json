{
  "manifest_version": 3,
  "name": "SymplyKode",
  "description": "Summarizes code",
  "version": "1.0.0",
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "permissions": [
      "scripting",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "https://*/"
  ]
}
